<!DOCTYPE html>
<html>
<head>
  <title>HYPRACTV</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
	<%= render :partial => 'layouts/ga' if Rails.env.production? %>
	<%= csrf_meta_tags %>
</head>
<body>
	  <%= render 'layouts/header' %>
		<%= '<div class="container"> <p id="notice" class="alert-message success"> '.html_safe + notice + '</p></div>'.html_safe if ( !notice.nil? && !notice.blank? )%>
	  <%= yield %>
		<%= render 'layouts/footer' %>
		<%= yield :scripts %>
		<%= debug(params) if Rails.env.development? %>
		<%= '<br/>'.html_safe if Rails.env.development? %>
		<%= notice if Rails.env.development? %>
</body>
</html>


